<!DOCTYPE HTML>
<html>
  <head>
    <title>Day Tracker</title>
  </head>
  <body>
    <table>
      <tr>
        <th>Time</th>
        <th>Time Passed</th>
        <th>Remove</th>
      </tr>

      <% for(var i = 0; i <= results.length - 1; i++){ %>
        <tr>
          <td><%= results[i].time_checked %></td>
          <td><%= results[i].time_passed %></td>
          <td>
            <form action='/deleteTime/<%= results[i].id %>'>
              <button>Delete</button>
            </form>
          </td>
        </tr>
      <% } %>
    </table>
  </body>
</html>
